package org.osmf.layout
{
	/**
	 * Defines an enumeration of layout modes as supported by the
	 * layout renderer.
	 *  
	 *  @langversion 3.0
	 *  @playerversion Flash 10
	 *  @playerversion AIR 1.5
	 *  @productversion OSMF 1.0
	 */	
	public final class LayoutMode
	{
		/**
		 * Identifier for the layout renderer's default mode.
		 *  
		 *  @langversion 3.0
		 *  @playerversion Flash 10
		 *  @playerversion AIR 1.5
		 *  @productversion OSMF 1.0
		 */
		public static const NONE:String = "none";
		
		/**
		 * Identifier for the layout renderer's vertical mode.
		 *  
		 *  @langversion 3.0
		 *  @playerversion Flash 10
		 *  @playerversion AIR 1.5
		 *  @productversion OSMF 1.0
		 */
		public static const VERTICAL:String = "vertical";
		
		/**
		 * Identifier for the layout renderer's horizontal mode.
		 *  
		 *  @langversion 3.0
		 *  @playerversion Flash 10
		 *  @playerversion AIR 1.5
		 *  @productversion OSMF 1.0
		 */
		public static const	HORIZONTAL:String = "horizontal";
		
		public static const OVERLAY:String = "overlay";
	}
}